{
  "type": "mongodb",
  "host": "localhost",
  "database": "smarthouse",
  "synchronize": true,
  "logging": false,
  "entities": ["src/models/*.ts"],
  "migrations": ["src/migration/*.ts"],
  "cli": {
    "entitiesDir": "src/models",
    "migrationsDir": "src/migration"
  },
  "useNewUrlParser": true,
  "useUnifiedTopology": true
}
